    "version": "2.0.0",
    "tasks": [
      {
        "label": "Abrir en Chrome",
        "command": "chrome",
        "windows": {
          "command": "C:/Program Files (x86)/Google/Chrome/Application/chrome.exe"
        },
        "args": [
          "http://localhost:8080/${workspaceRootFolderName}/${relativeFile}"
        ],
        "presentation": {
          "reveal": "never"
        },
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
}